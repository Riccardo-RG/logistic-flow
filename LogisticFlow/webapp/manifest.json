{
  "_version": "1.12.0",
  "sap.app": {
    "id": "logistic-flow",
    "type": "application",
    "applicationVersion": {
      "version": "0.0.1"
    }
  },
  "sap.ui5": {
    "dependencies": {
      "minUI5Version": "1.60",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "models": {
      "": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "model/data.json",
        "settings": {}
      }
    },
    "rootView": {
      "viewName": "logistic-flow.view.App",
      "type": "XML",
      "id": "App"
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "logistic-flow.view",
        "controlId": "navContainer",
        "controlAggregation": "pages",
        "async": true
      },
      "routes": [
        {
          "pattern": "",
          "name": "MainView",
          "target": "MainView"
        },
        {
          "pattern": "BP-Preparation",
          "name": "BP-Preparation",
          "target": "BP-Preparation"
        },
        {
          "pattern": "BP-RechercheProduit",
          "name": "BP-RechercheProduit",
          "target": "BP-RechercheProduit"
        },
        {
          "pattern": "BP-DoJour",
          "name": "BP-DoJour",
          "target": "BP-DoJour"
        },
        {
          "pattern": "DetailDuClient",
          "name": "DetailDuClient",
          "target": "DetailDuClient"
        }
      ],
      "targets": {
        "MainView": {
          "viewName": "MainView",
          "viewType": "XML",
          "controlId": "navContainer",
          "controlAggregation": "pages"
        },
        "BP-Preparation": {
          "viewName": "BP-Preparation",
          "viewType": "XML",
          "controlId": "navContainer",
          "controlAggregation": "pages"
        },
        "BP-RechercheProduit": {
          "viewName": "BP-RechercheProduit",
          "viewType": "XML",
          "controlId": "navContainer",
          "controlAggregation": "pages"
        },
        "BP-DoJour": {
          "viewName": "BP-DoJour",
          "viewType": "XML",
          "controlId": "navContainer",
          "controlAggregation": "pages"
        },
        "DetailDuClient": {
          "viewName": "DetailDuClient",
          "viewType": "XML",
          "controlId": "navContainer",
          "controlAggregation": "pages"
        }
      }
    }
  }
}
