{
  "_version": "1.12.0",
  "sap.app": {
    "id": "logistic-flow",
    "type": "application",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "i18n": "i18n/i18n.properties"
  },
  "sap.ui5": {
    "dependencies": {
      "minUI5Version": "1.60",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "models": {
      "": {
        "dataSource": "mainService",
        "settings": {
          "preload": true,
          "sync": true
        }
      }
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZPP_WSM_SRV/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0",
          "useBatch": false,
          "defaultBindingMode": "TwoWay",
          "defaultCountMode": "Inline",
          "json": true,
          "headers": {
            "Accept": "application/json"
          }
        }
      }
    },
    "rootView": {
      "viewName": "logistic-flow.view.App",
      "type": "XML",
      "id": "App"
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "logistic-flow.view",
        "controlId": "navContainer",
        "controlAggregation": "pages",
        "async": true
      },
      "routes": [
        {
          "pattern": "",
          "name": "MainView",
          "target": "MainView"
        },
        {
          "pattern": "BP-Preparation",
          "name": "BP-Preparation",
          "target": "BP-Preparation"
        },
        {
          "pattern": "BP-RechercheProduit",
          "name": "BP-RechercheProduit",
          "target": "BP-RechercheProduit"
        },
        {
          "pattern": "BP-DoJour",
          "name": "BP-DoJour",
          "target": "BP-DoJour"
        },
        {
          "pattern": "DetailDuClient",
          "name": "DetailDuClient",
          "target": "DetailDuClient"
        },
        {
          "pattern": "PrepareProduit",
          "name": "PrepareProduit",
          "target": "PrepareProduit"
        },
        {
          "pattern": "Batch",
          "name": "Batch",
          "target": "Batch"
        },
        {
          "pattern": "Manage",
          "name": "Manage",
          "target": "Manage"
        }
      ],
      "targets": {
        "MainView": {
          "viewName": "MainView",
          "viewType": "XML",
          "controlId": "navContainer",
          "controlAggregation": "pages"
        },
        "BP-Preparation": {
          "viewName": "BP-Preparation",
          "viewType": "XML",
          "controlId": "navContainer",
          "controlAggregation": "pages"
        },
        "BP-RechercheProduit": {
          "viewName": "BP-RechercheProduit",
          "viewType": "XML",
          "controlId": "navContainer",
          "controlAggregation": "pages"
        },
        "BP-DoJour": {
          "viewName": "BP-DoJour",
          "viewType": "XML",
          "controlId": "navContainer",
          "controlAggregation": "pages"
        },
        "DetailDuClient": {
          "viewName": "DetailDuClient",
          "viewType": "XML",
          "controlId": "navContainer",
          "controlAggregation": "pages"
        },
        "PrepareProduit": {
          "viewName": "PrepareProduit",
          "viewType": "XML",
          "controlId": "navContainer",
          "controlAggregation": "pages"
        },
        "Batch": {
          "viewName": "Batch",
          "viewType": "XML",
          "controlId": "navContainer",
          "controlAggregation": "pages"
        },
        "Manage": {
          "viewName": "HandlingUnitManagement",
          "viewType": "XML",
          "controlId": "navContainer",
          "controlAggregation": "pages"
        }
      }
    }
  }
}
